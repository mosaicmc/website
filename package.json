{
  "name": "mosaic-multicultural-connections",
  "private": true,
  "version": "0.1.0-alpha.0",
  "type": "module",
  "scripts": {
    "prebuild": "tsc scripts/generate-search-index.ts --outDir scripts/dist --module ESNext --target ES2020 --moduleResolution node --skipLibCheck --types node && node scripts/dist/generate-search-index.js",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "smoke": "sh -c 'vite preview --host 127.0.0.1 --port 4173 & PREVIEW_PID=$!; sleep 10; node -e \"(async()=>{const net=require(\\\"net\\\");const s=net.connect(4173,\\\"127.0.0.1\\\",()=>{s.end();process.exit(0);});s.on(\\\"error\\\",()=>process.exit(1));})();\"; EXIT=$?; kill $PREVIEW_PID; exit $EXIT'",
    "validate": "npm run lint && npm run typecheck && npm run test && npm run build && npm run smoke",
    "preview": "vite preview --host 127.0.0.1",
    "scrape:reviews": "ts-node --esm --transpile-only scripts/scrape-google-reviews.ts",
    "scrape:reviews:xpath": "ts-node --esm --transpile-only scripts/scrape-google-reviews-xpath.ts",
    "scrape:reviews:custom": "ts-node --esm --transpile-only scripts/scrape-google-reviews-custom.ts",
    "verify:reviews": "ts-node --esm --transpile-only scripts/verify-google-reviews.ts",
    "audit:aus": "node scripts/audit-aus-spelling.js",
    "ingest:testimonials:md": "ts-node --esm --transpile-only scripts/ingest-testimonials-from-md.ts",
    "test": "vitest",
    "test:ui": "playwright test",
    "test:puppeteer": "node tests/section-order.puppeteer.cjs",
    "scan:secrets": "node scripts/scan-secrets.cjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.18.1",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.344.0",
    "motion": "^12.18.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.68.0",
    "react-i18next": "^15.5.3",
    "react-router-dom": "^6.20.1",
    "tailwind-merge": "^2.6.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.56.1",
    "@types/node": "^20.19.25",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "wait-on": "^9.0.3"
  },
  "optionalDependencies": {
    "devtools-protocol": "0.0.1558402",
    "puppeteer": "^22.15.0",
    "sanitize-html": "^2.17.0"
  },
  "engines": {
    "node": ">=20"
  }
}
